st fetchAuthenticated = () => fetchFromAccount({\r\n  endpoint: 'authenticated',\r\n  options: { credentials: 'include' },\r\n  SUCCESS_TYPE: ACCOUNT.FETCH_AUTHENTICATED_SUCCESS\r\n});"},"lineCount":null}},"error":null,"hash":"eb40be8795295aa5bf0d85328f5c18e7","cacheData":{"env":{}}}